//Largest element in array 

class Main {
    public static void main(String[] args) {
        int arr[]={2,4,5,6,8};
        int largest=arr[0];
        for(int i=0;i<arr.length;i++){
            if (arr[i] > largest)
            largest = arr[i];

        }
        System.out.print(largest);
    
    }
}
